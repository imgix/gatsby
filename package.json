{
  "version": "1.0.0-rc.2",
  "name": "@imgix/gatsby",
  "description": "A Gatsby plugin to integrate with imgix's APIs",
  "author": "Frederick Fogerty <frederick@imgix.com>",
  "license": "BSD-2-Clause",
  "repository": "https://github.com/imgix/gatsby.git",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Angelo Ashmore <angelo.ashmore@walltowall.com>"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --build --watch",
    "lint": "prettier --write .",
    "lint:ci": "prettier --check .",
    "test": "run-s test:unit",
    "test:unit": "yarn jest",
    "test:e2e": "yarn workspace dev-and-e2e-test-environment run test:e2e",
    "test:e2e:ci": "yarn workspace dev-and-e2e-test-environment run test:e2e:ci",
    "tdd": "run-p tdd:unit tdd:e2e",
    "tdd:unit": "yarn jest --watch --setupFiles dotenv/config",
    "tdd:e2e": "yarn test:e2e"
  },
  "peerDependencies": {
    "gatsby-image": "^2.4.12"
  },
  "dependencies": {
    "@types/jsuri": "^1.3.30",
    "@types/node-fetch": "^2.5.7",
    "@types/ramda": "^0.27.14",
    "camel-case": "^4.1.1",
    "debug": "^4.2.0",
    "fp-ts": "^2.8.2",
    "fp-ts-contrib": "^0.1.18",
    "imgix-core-js": "^2.3.1",
    "imgix-url-params": "^11.7.0",
    "io-ts": "^2.2.10",
    "io-ts-reporters": "^1.2.2",
    "jsuri": "^1.3.1",
    "node-fetch": "^2.6.0",
    "ramda": "^0.27.1",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@google/semantic-release-replace-plugin": "^1.0.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.15",
    "cross-env": "^7.0.2",
    "gatsby": "^2.26.0",
    "gatsby-image": "^2.5.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.11.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "read-pkg": "^5.2.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "yarn-run-all": "^3.1.1"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": "rc"
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "src/**",
            "dist/**",
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes} [skip ci]"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ]
    ]
  },
  "keywords": [
    "blur-up",
    "cdn",
    "fixed",
    "fluid",
    "gatsby-image",
    "gatsby-plugin",
    "gatsby",
    "image",
    "imgix",
    "optimise",
    "optimize",
    "placeholder",
    "responsive",
    "transform",
    "webp"
  ]
}
