{
  "name": "dev-and-e2e-test-environment",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.6",
  "author": "",
  "dependencies": {
    "@imgix/gatsby": "^1.0.0-rc.1",
    "gatsby": "^2.29.0",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-manifest": "^2.9.0",
    "gatsby-plugin-offline": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sharp": "^2.11.0",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.2",
    "cypress": "^6.1.0",
    "prettier": "2.2.1",
    "read-pkg-up": "^7.0.1",
    "start-server-and-test": "^1.11.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm run test:e2e:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "./node_modules/.bin/start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "./node_modules/.bin/start-server-and-test develop http://localhost:8000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
