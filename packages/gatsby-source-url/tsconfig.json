{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "outDir": "dist",
    "rootDir": "src",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node"],
    "baseUrl": "./",
    "paths": {
      "*": ["src/*", "node_modules/*", "../../node_modules"]
    },
    "resolveJsonModule": true
  },
  "include": ["./src/**/*", "types"],
  "exclude": ["./node_modules/*"]
}
